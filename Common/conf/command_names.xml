<?xml version="1.0" encoding="UTF-8"?>
<commands>
	<!-- comments -->
	<command_name name="acquire">
        <alias>a, acq</alias>
		
		<properties>
			<feature name="exptime">
				<alias>e</alias>
				<variable_type>float</variable_type>
				<default>15.0</default>
			</feature>
			
			<feature name="discard">
				<alias>d</alias>
				<variable_type>int</variable_type>
				<default>0</default>
			</feature>
			
			<feature name="save">
				<alias>s</alias>
				<variable_type>int</variable_type>
				<default>0</default>
			</feature>
		</properties>
    </command_name>
	
	<command_name name="exit">
        <alias>e</alias>
    </command_name>
</commands>